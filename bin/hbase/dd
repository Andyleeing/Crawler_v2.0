nsive = true;        if (Q.browser.IE6 || Q.browser.IE7) {          // IE6/7没有响应式          isResponsive = false;        }        var wrapper = document.createElement('div');        var element = document.createElement('div');        var ifr = document.createElement('iframe');                var wrapperMinW = wminWidth || 1000;        var elementMinW = minWidth || 980;        var elementMaxW = width;        var elementW = '100%';        if (!isResponsive) {          // 非响应式则设定为固定宽度          elementW = elementMinW + 'px';        }                wrapper.id = 'crazy' + crazyId;        wrapper.style.cssText = 'position:absolute;left:0px;top:-5000px;z-index:3;width:100%;height:' + height + 'px;min-width:' + wrapperMinW + 'px;';                element.id = 'flash' + crazyId;        element.style.cssText = 'position:relative;z-index:3;width:' + elementW + ';height:100%;margin:0 auto;max-width:' + elementMaxW +'px;min-width:' + elementMinW + 'px;';        element.innerHTML = createSwfObject(crazyUrl, crazyId, '100%', '100%');                ifr.style.cssText = 'left:0px;top:0px;width:100%;height:100%;position:absolute;z-index:1;background-color:#000';                element.appendChild(ifr);        wrapper.appendChild(element);        document.body.appendChild(wrapper);    }    function createSwfObject(url, crazyId, width, height) {        var wm = 'opaque';        if (Q.browser.SAFARI) {            wm = 'window';        }        var opts = {};        opts.id = 'adflash' + crazyId;        opts.width = width;        opts.height = height;        opts.params = {            'wMode': wm        };        opts.vars = {            'playerId': 'flash',            'creativeId': crazyId        };        opts.styles = {            'position': 'absolute',            'z-index': '2'        };                return Q.flash.create(url, opts);                }    function showCrazy(crazyId) {              var top = Q.$('#flash').top() + 1; // 错位1像素可使底下的播放器露出来从而避免在FF等浏览器上因flash被遮盖而代码不执行的bug，UI上不影响        var wrapper = document.getElementById('crazy' + crazyId);        if (wrapper) {            wrapper.style.top = top + 'px';        }    }        function resizeCrazy(crazyId, width, height) {        var wrapper = document.getElementById('crazy' + crazyId);        var element = document.getElementById('flash' + crazyId);        if (wrapper) {          wrapper.style.height = height + 'px';        }        if (element) {          element.style.maxWidth = width + 'px';        }    }    function closeCrazy(crazyId) {        var wrapper = document.getElementById('crazy' + crazyId);        if (wrapper) {            wrapper.innerHTML = '';            document.body.removeChild(wrapper);        }    }</script><!-- 疯狂前贴 End -->      <div class="container-long container_v1 ">        <!--功能区-->        <div id="block-C" data-block-name="功能区" data-pb="tmplt=longmovietplt">          <div class="leftShare leftShare0116">            <div class="func_panels func_panels_new">               <div style="display:none;" data-funcareafloater-elem="phone" class="func_panel panel-view" data-private-display="block">  <div class="popup-phoneView phoneViewNew">    <div class="clearfix">      <div class="phoneView">        <h2>拍下二维码，用手机继续看</h2>        <p class="mb15">帮助：          <a title="如何使用二维码" target="_blank" rel="nofollow" href="http://www.iqiyi.com/common/helpandsuggest.html#wenti18"><span class="green">如何使用二维码</span></a>        </p>        <p><img width="64" height="64" alt="如何使用二维码" src="http://qrcode.qiyipic.com/qrcoder/?data=http%3A%2F%2Fwww.iqiyi.com%2Fv_19rrh5cwhw.html%3Fqr%3Dpage&amp;&amp;salt=05d20b4503bb9a6b1f19fb673f6508ee&amp;property=0" id="data-watchbyphone-image"></p>      </div>      <div class="phone-Rcon">        <div class="phone-Rcon-t">          <p>下载移动客户端随时随地观看</p>          <p><span id="data-watchbyphone-title">小鞋子</span>更多精彩内容</p>        </div>        <div class="phone-Bcon phone-Bcon-mobile-pop">          <div class="phone-Bcon-l">            <p class="ph